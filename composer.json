{
  "name": "mascame/artificer",
  "description": "Artificer is an admin package built on top of your Eloquent models.",
  "homepage": "https://github.com/marcmascarell/laravel-artificer",
  "keywords": [
    "laravel",
    "admin",
    "artificer",
    "admin interface",
    "eloquent admin",
    "administration"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Marc Mascarell",
      "email": "marcmascarell@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "laravelcollective/html": "5.*",
    "mascame/artificer-default-theme": "dev-l5-migration",
    "mascame/artificer-widgets": "dev-l5-migration",
    "mascame/formality": "0.*",
    "mascame/extender": "dev-master",
    "mascame/notify": "1.*"
  },
  "require-dev": {
    "sami/sami": "3.*",
    "codeception/codeception": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Mascame\\Artificer\\": "src/"
    }
  },
  "minimum-stability": "dev"
}
